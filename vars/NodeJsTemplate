def call(Map config) {

 node('master'){
 sh 'ls'
stage('Checkout'){
    echo 'Checking out Code from GitHub'
        checkout scm
}
stage('Build'){

    echo 'building'
    sh 'npm install'
}
stage('Test'){ 
    echo 'Testing'  
}
stage('code analysis'){

  echo 'Starting Code Analysis'
  }

stage('Publish') { 
  sh libraryResource 'docker-push.sh'
 sh request
 }
stage('Post Action'){
echo 'Cleaning Work Space'
echo 'Sending email'
}
}


}

